      "url": "http://localhost:3000",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///../../server/pages/*.tsx":
          "${workspaceFolder}/server/pages/*.tsx",
        "webpack:///src/*": "${webRoot}/*",
        "webpack:///*": "*",
        "webpack:///./~/*": "${webRoot}/node_modules/*",
        "meteor://ðŸ’»app/*": "${webRoot}/*"
